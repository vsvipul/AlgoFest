// C++ program to demonstrate  
// accessing of data members  
  
#include <bits/stdc++.h> 
using namespace std; 
class Student 
{ 
    // Access specifier 
    public: 
  
    // Data Members 
    string name; 
  
    // Member Functions() 
    void printname() 
    { 
       cout << "Name is: " << name; 
    } 
}; 
  
int main() { 
  
    // Declare an object of class Student 
    Student obj1; 
  
    // accessing data member 
    obj1.name = "neha"; 
  
    // accessing member function 
    obj1.printname(); 
    return 0; 
} 
